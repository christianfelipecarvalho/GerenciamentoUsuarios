@{
    ViewData["Title"] = "Página Inicial";
}

<div class="text-center">
    <h1 class="display-4">Bem vindo(a)</h1>
    <form >
        <h1 style="margin-left:-10px;">Login</h1>
        <label style="margin-left:-10px;text-align:right;display:inline-block;" for="username">Usuário:</label>
        <input style="text-align:left;display:inline-block;" type="text" id="username" ng-model="username" required>
        <br></br>
        <label style="margin-left:-10px;text-align:right;display:inline-block;" for="password">Senha:</label>
        <input style="text-align:left;display:inline-block;" type="password" id="password" ng-model="password" required>
        <br></br>
        <input type="submit" value="Entrar" ng-click="submit()">
    </form>
    
</div>
